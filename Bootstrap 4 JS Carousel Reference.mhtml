.gs-result .gs-title, .gs-result .gs-title * { color: rgb(17, 85, 204); }

.gs-result a.gs-visibleUrl, .gs-result .gs-visibleUrl { color: rgb(0, 153, 51); text-decoration: none; padding-bottom: 2px; }

.gsc-results .gsc-cursor-box { margin: 10px; }

